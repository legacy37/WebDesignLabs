print("5.1")
a, b = map(float, input().split())
print(a if a > b else b)
print("Задание 5.2")
word = input().lower()
if word == word[::-1]:
    print("Palindrome")
else:
    print("Not a Palindrome")
print("Задание 5.3")
a, b = map(int, input().split())
if a % b == 0:
    print(a // b)
else:
    print(f"{a} на {b} нацело не делится")
print("Задание 5.4")
a, b, c = map(int, input().split())
if a**2 + b**2 == c**2:
    print("Yes")
else:
    print("No")
print("Задание 5.5")
number = int(input())
if number % 10 == 7:
    print("Yes")
else:
    print("No")
print("Задание 5.6")
word = input().lower()
if 't' in word and 'h' in word and 'o' in word:
    print("Yes")
else:
    print("No")
print("Задание 5.7")
cities = input().split()
if 'Москва' in cities:
    cities.remove('Москва')
print(' '.join(cities))
print("Задание 5.8")
a, b, c, d = map(int, input().split())
if (c + 2 <= a and d + 2 <= b) or (c + 2 <= b and d + 2 <= a):
    print("Yes")
else:
    print("No")
print("Задание 5.9")
number = input()
if sum(map(int, number[:3])) == sum(map(int, number[3:])):
    print("Yes")
else:
    print("No")
print("Задание 5.10")
time = float(input())
cycle = time % 5
if cycle < 3:
    print("green")
else:
    print("red")
print("Задание 5.11")
choice = int(input())
if choice == 1:
    print("Введение в Python")
elif choice == 2:
    print("Строки и списки")
elif choice == 3:
    print("Условные операторы")
elif choice == 4:
    print("Циклы")
elif choice == 5:
    print("Словари, кортежи и множества")
elif choice == 6:
    print("Выход")
print("Задание 5.12")
a, b, c = map(int, input().split())
if a <= b and a <= c:
    print(a)
elif b <= a and b <= c:
    print(b)
else:
    print(c)
