day = int(input())
if day == 1:
    print("понедельник")
elif day == 2:
    print("вторник")
elif day == 3:
    print("среда")
elif day == 4:
    print("четверг")
elif day == 5:
    print("пятница")
elif day == 6:
    print("суббота")
elif day == 7:
    print("воскресенье")
print("Задание 5.15")
month = int(input())
if month in [1, 3, 5, 7, 8, 10, 12]:
    print(31)
elif month in [4, 6, 9, 11]:
    print(30)
else:
    print(28)
print("Задание 5.16")
d, m = map(int, input().split())
days_in_month = [31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31]

if d == 1:
    prev_m = m - 1
    prev_d = days_in_month[prev_m - 1]
else:
    prev_m = m
    prev_d = d - 1

if d == days_in_month[m - 1]:
    next_m = m + 1 if m < 12 else 1
    next_d = 1
else:
    next_m = m
    next_d = d + 1

print(f"{prev_d:02d}.{prev_m:02d} {next_d:02d}.{next_m:02d}")
print("Задание 5.17")
k = int(input())
days_of_week = ["понедельник", "вторник", "среда", "четверг", "пятница", "суббота", "воскресенье"]
day_index = (k - 1) % 7
print(days_of_week[day_index])
