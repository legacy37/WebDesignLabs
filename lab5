print("Задание 5.23")
temp = int(input())
scale = input().upper()

if scale == 'C':
    result = int(temp * 1.8 + 32)
elif scale == 'F':
    result = int((temp - 32) / 1.8)
else:
    result = "Неправильно введены градусы"

print(result)
print("Задание 5.24")
letter = input().lower()
vowels_ru = "аоуиыэеёюя"
vowels_en = "aeiou"
consonants_ru = "бвгджзйклмнпрстфхцчшщ"
consonants_en = "bcdfghjklmnpqrstvwxyz"

if letter in vowels_ru or letter in vowels_en:
    print("Гласная")
elif letter in consonants_ru or letter in consonants_en:
    print("Согласная")
elif letter in "ъь":
    print(letter)
else:
    print("Неправильно введена буква")
print("Задание 5.25")
size = input().upper()
age = int(input())

if size == 'М':
    human_age = 16 * age + 31 if age > 2 else 10.5 * age
elif size == 'С':
    human_age = 12 * age + 31 if age > 2 else 10.5 * age
elif size == 'К':
    human_age = 8 * age + 31 if age > 2 else 10.5 * age
else:
    human_age = "Неправильно введен размер"

print(human_age)